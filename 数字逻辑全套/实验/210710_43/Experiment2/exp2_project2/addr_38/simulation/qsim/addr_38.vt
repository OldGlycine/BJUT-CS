`timescale 1 ps/ 1 ps
module addr_38_vlg_sample_tst(
	addr,
	sampler_tx
);
input [2:0] addr;
output sampler_tx;

reg sample;
time current_time;
always @(addr)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
 begin                                                
	if ($realtime == 0 || $realtime != current_time)  
	begin									          
		if (sample === 1'bx)                          
			sample = 0;                               
		else                                          
			sample = ~sample;                         
	end										          
	current_time = $realtime;					      
 end                                                  
end                                                   

assign sampler_tx = sample;
endmodule

